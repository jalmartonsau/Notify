<Window x:Class="Notify.Views.Insert"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notify.Views"
        xmlns:vm="clr-namespace:Notify.Model"
        mc:Ignorable="d"
        WindowStyle="None"
        ResizeMode="NoResize"
        Background="#2f2f2f"
        Title="Insert" Height="100" Width="300">
    <Window.DataContext>
        <vm:InsertVM/>
    </Window.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <TextBox Grid.ColumnSpan="2" DockPanel.Dock="Top" Text="{Binding Insert}" />
        <Button Grid.Row="1" Command="{Binding TestCommand, Mode=OneWay}"  >Hello</Button>
    </Grid>
</Window>
